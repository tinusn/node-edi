{
	// http://www.jshint.com/docs/options/
	// Enforcing options
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"eqeqeq": true,
	"es3": false,
	"es5": true,
	"forin": true,
	"freeze": true,
	"immed": false,
	"indent": 4,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonew": true,
	"plusplus": false,
	"quotmark": "single",
	"undef": true,
	"unused": true,
	"strict": true,
	"trailing": true,
	// Relaxing options
	"asi": false,
	"boss": false,
	"debug": true,
	"eqnull": false,
	"esnext": false,
	"evil": false,
	"expr": false,
	"funcscope": false,
	"globalstrict": false,
	"iterator": false,
	"lastsemic": false,
	"laxbreak": false,
	"laxcomma": false,
	"loopfunc": false,
	"multistr": false,
	"notypeof": false,
	"proto": false,
	"scripturl": false,
	"smarttabs": true,
	"shadow": false,
	"sub": true,
	"supernew": false,
	"validthis": false,
	"regexp": true,
	// Environments
	"browser": true,
	"devel": false,
	"node": true,
	"globals": {
		"angular": false,
		"process": false,
		"require": false,
		"Buffer"
	}
}
